<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="Theme.AppSplash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/app_green</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher_round</item>
        <item name="postSplashScreenTheme">@style/Theme.FiveOClockSomewhere</item>

        <!-- Status bar and Nav bar configs -->
        <item name="android:statusBarColor">@color/app_green</item>
        <item name="android:navigationBarColor">@color/app_green</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
</resources>